{
  "name": "learning-redux-saga-material-ui-redux-form",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.3",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "query-string": "^6.11.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.0.1",
    "react-toastify": "^5.5.0",
    "redux": "^4.0.5",
    "redux-form": "^8.3.0",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "export PORT=4000 && react-scripts start",
